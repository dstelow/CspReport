<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <httpProtocol>
      <customHeaders>
        <add name="Content-Security-Policy" 
             value="default-src 'self'; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; img-src 'self' data:; font-src 'self'; connect-src 'self'; report-uri http://claritygroupdev.northcentralus.cloudapp.azure.com/csp/report-uri;" />
      </customHeaders>
    </httpProtocol>
  </system.webServer>
</configuration>

{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Csp": {
    "ReportUri": "/csp-report-uri",
    "ReportOnly": false
  },
  
  "CspReport": {
    "LogDirectory": "Logs",
    "FileName": "csp-report-uri.jsonl",
    "MaxBodyBytes": 262144
  }
  
}
